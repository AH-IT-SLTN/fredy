{
  "name": "fredy",
  "version": "2.0.0",
  "description": "[F]ind [R]eal [E]states [d]amn eas[y].",
  "scripts": {
    "start": "node index.js",
    "format": "prettier --write lib/**/*.js test/**/*.js *.js --single-quote --print-width 120",
    "test": "mocha --timeout 20000 test/**/*.test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint ./index.js ./lib/**/*.js ./test/**/*.js",
      "prettier --single-quote --print-width 120 --write"
    ]
  },
  "main": "index.js",
  "author": "Christian Kellner",
  "keywords": [
    "flat",
    "flatfinder",
    "fredy",
    "real estates",
    "germany",
    "apartment",
    "house",
    "rent",
    "immoscout",
    "immonet",
    "immowelt",
    "immobilienscout24"
  ],
  "bugs": {
    "url": "https://github.com/orangecoding/fredy/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=11.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.3",
    "body-parser": "1.19.0",
    "handlebars": "^4.7.6",
    "lowdb": "1.0.0",
    "node-mailjet": "^3.3.1",
    "request-x-ray": "0.1.4",
    "restana": "4.7.2",
    "slack": "11.0.2",
    "tg-yarl": "1.3.0",
    "x-ray": "2.3.4"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "7.7.0",
    "eslint-config-prettier": "6.11.0",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "mocha": "8.1.1",
    "prettier": "2.0.5",
    "proxyquire": "2.1.3"
  }
}
